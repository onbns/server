{
  "name": "onbns-server",
  "version": "0.0.1",
  "description": "",
  "main": "build/server.js",
  "scripts": {
    "lint": "eslint .",
    "lint:fix": "eslint --fix .",
    "precommit": "lint-staged",
    "prepush": "npm run lint:fix",
    "dev": "babel-watch src/server.js",
    "start": "NODE_ENV=production node build/server.js",
    "build": "rm -rf ./build && NPM_CONFIG_PRODUCTION=false babel -d ./build ./src -s"
  },
  "author": "amazingandyyy",
  "lint-staged": {
    "*.js": [
      "npm run lint:fix",
      "git add"
    ]
  },
  "babel": {
    "presets": [
      "es2015",
      "env"
    ],
    "plugins": [
      "transform-object-rest-spread"
    ],
    "ignore": [
      "node_modules"
    ]
  },
  "dependencies": {
    "aws-sdk": "^2.205.0",
    "axios": "^0.17.1",
    "bcrypt-nodejs": "^0.0.3",
    "body-parser": "^1.15.2",
    "cheerio": "^1.0.0-rc.2",
    "cors": "^2.8.1",
    "crypto-js": "^3.1.9-1",
    "debug": "^4.1.0",
    "dotenv": "^2.0.0",
    "express": "^4.16.2",
    "jwt-simple": "^0.5.1",
    "mongoose": "^5.3.12",
    "multer": "^1.3.0",
    "node-ses": "^2.1.0",
    "qs": "^6.5.1",
    "raven": "^2.4.1",
    "request": "^2.88.0"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "babel-watch": "^2.0.7",
    "eslint": "^4.14.0",
    "eslint-config-logux": "^17.0.0",
    "eslint-config-standard": "^10.2.1",
    "eslint-plugin-babel": "^5.2.1",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-jest": "^21.5.0",
    "eslint-plugin-node": "^5.2.1",
    "eslint-plugin-promise": "^3.6.0",
    "eslint-plugin-react": "^7.5.1",
    "eslint-plugin-security": "^1.4.0",
    "eslint-plugin-standard": "^3.0.1",
    "husky": "^1.1.4",
    "lint-staged": "^8.0.5",
    "morgan": "^1.9.0"
  }
}
